{
  "name": "gantt-chart-generator",
  "version": "1.0.0",
  "description": "A desktop app for generating a Gantt Chart from timestamps.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . GanttChart --overwrite --asar=true --platform=win32 --arch=x64 --icon=app_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Gantt Chart\""
  },
  "keywords": [
    "Electron",
    "Gantt",
    "Chart"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron": "^29.1.5"
  },
  "devDependencies": {
    "electron-packager": "^17.1.2"
  }
}